<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="core._V.TextItem_V">
  <Grid ColumnDefinitions="1*, 1*">
    <TextBlock Grid.Column="0" Text="{Binding TextItem.Label, Mode=OneWay}" FontSize="20" VerticalAlignment="Center"/>
    <TextBox Grid.Column="1" Text="{Binding TextItem.Val, Mode=TwoWay}" FontSize="20" VerticalAlignment="Center"/>
  </Grid>
</UserControl>
