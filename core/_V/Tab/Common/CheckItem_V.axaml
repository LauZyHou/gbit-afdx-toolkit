<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="core._V.CheckItem_V">
  <Grid ColumnDefinitions="1*, 1*">
    <TextBlock Grid.Column="0" Text="{Binding CheckItem.Label, Mode=OneWay}" VerticalAlignment="Center"/>
    <CheckBox Grid.Column="1" IsChecked="{Binding CheckItem.Val, Mode=TwoWay}" IsThreeState="False" VerticalAlignment="Center"/>
  </Grid>
</UserControl>
